{
  "name": "baking-react-ts",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bootstrap": "4.0.0",
    "classnames": "^2.2.5",
    "create-react-class": "^15.6.2",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "moment": "^2.20.1",
    "moment-timezone": "^0.5.14",
    "node-sass-chokidar": "^0.0.3",
    "npm-run-all": "^4.1.2",
    "prop-types": "^15.6.0",
    "react": "^16.1.1",
    "react-create-class": "^1.0.0",
    "react-data-grid": "^3.0.3",
    "react-data-grid-addons": "^3.0.3",
    "react-dom": "^16.1.1",
    "react-redux": "^5.0.6",
    "react-redux-typescript": "^3.0.0-beta1",
    "react-scripts-ts": "2.12.0",
    "react-table": "^6.7.6",
    "react-transition-group": "^2.2.1",
    "reactstrap": "^5.0.0-alpha.4",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "whatwg-fetch": "^2.0.3"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src/styles --include-path ./node_modules src/styles/ -o src/styles/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src/styles --include-path ./node_modules src/styles/ -o src/styles --watch --recursive",
    "start-js": "react-scripts-ts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.3",
    "@types/enzyme": "^3.1.6",
    "@types/enzyme-adapter-react-16": "^1.0.1",
    "@types/fetch-mock": "^5.12.2",
    "@types/jest": "^22.0.1",
    "@types/moment-timezone": "^0.5.3",
    "@types/node": "^9.3.0",
    "@types/react": "^16.0.25",
    "@types/react-data-grid": "^2.0.7",
    "@types/react-dom": "^16.0.3",
    "@types/react-redux": "^5.0.13",
    "@types/react-table": "^6.7.0",
    "@types/reactstrap": "^5.0.3",
    "@types/redux-logger": "^3.0.5",
    "@types/redux-mock-store": "^0.0.13",
    "fetch-mock": "^6.0.0-beta.7",
    "jsdom": "^11.5.1",
    "jsdom-global": "^3.0.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-mock-store": "^1.4.0"
  }
}
